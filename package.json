    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-runtime": "^6.20.0",
    "chalk": "^2.4.1",
    "css-loader": "^1.0.0",
    "enzyme": "^3.7.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",

    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "comma-separated-values": "^3.6.4",
    "mammoth": "1.4.6",
    "pdfjs-dist": "2.0.489",
    "prop-types": "^15.5.10",
    "react": "^16.5.2",
    "react-data-grid": "^4.0.8",
    "react-dom": "^16.5.2",
    "react-visibility-sensor": "4.1.3",
    "three": "0.97.0",
    "xlsx": "^0.14.0"
